function test_repo()

% tests some core functions

%% fitting

try
    fit_functions.fit_model_v2('grohn', 'RW')
catch
    helper_functions.cprintf('green', ''!!!! Fitting broken !!!!'\n')

    disp('!!!! Fitting broken !!!!')
    return
end
disp('Fitting: OK')


%% if all goes well

helper_functions.cprintf('green', 'all checks passed\n')

end